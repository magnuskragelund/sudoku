{
  "expo": {
    "name": "Sudoku Face Off",
    "slug": "sudoku",
    "version": "1.0.13",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sudokufaceoff",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "A modern Sudoku puzzle game with real-time multiplayer and AI-powered hints that teach you techniques. Play solo or challenge friends to solve puzzles together. Features smart hints that explain logic, visual highlighting, and context-aware explanations.",
    "primaryColor": "#2B7FFF",
    "backgroundColor": "#FFFFFF",
    "ios": {
      "bundleIdentifier": "com.sudokufaceoff.sudoku",
      "buildNumber": "2",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "supportsTablets": true,
      "package": "com.sudokufaceoff.sudoku",
      "versionCode": 13,
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
